function calculaForcaDaPassword(string) {
    let contagem = 0
if(string.length>= 8){
contagem += 1
}
for(let i=0;i<string.length;i++){
 if(typeof string[i] == "number") {
     contagem += 1
 }
 if ( string[i] == string[i].toUpperCase()){
     contagem += 1
 }
}
if (string == ""){
   return "Por favor introduza uma password vÃ¡lida."

}
return contagem
}
